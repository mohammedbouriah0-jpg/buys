{
  "cli": {
    "version": ">= 3.45.0"
  },
  "build": {
    "development": {
      "distribution": "internal",
      "android": {
        "buildType": "apk"
      },
      "ios": {
        "simulator": true
      },
      "env": {
        "EXPO_DEV": "1"
      }
    },
    "preview": {
      "distribution": "internal",
      "android": {
        "buildType": "apk"
      },
      "ios": {
        "simulator": false
      },
      "env": {
        "EXPO_PREVIEW": "1"
      }
    },
    "preview2": {
      "distribution": "internal",
      "android": {
        "buildType": "apk",
        "image": "latest"
      },
      "ios": {
        "simulator": false
      },
      "cache": {
        "disabled": true
      }
    },
    "production": {
      "android": {
        "buildType": "apk"
      },
      "ios": {
        "autoIncrement": true
      },
      "env": {
        "EXPO_PROD": "1"
      },
      "cache": {
        "disabled": true
      }
    }
  },
  "submit": {
    "production": {
      "android": {
        "serviceAccountKeyPath": "./google-service-account.json"
      },
      "ios": {
        "appleId": "VOTRE_APPLE_ID",
        "ascAppId": "VOTRE_APP_STORE_CONNECT_APP_ID"
      }
    }
  }
}
